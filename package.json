{
  "name": "financial-manager-app",
  "version": "1.0.0",
  "description": "Project to controls personal/profissional costs and receiveds.",
  "main": "main.js",
  "scripts": {
    "dev": "ng serve --port 4200 -c dev",
    "lan": "ng serve --port 4200 --host=0.0.0.0 --disable-host-check -c lan",
    "stg": "ng serve --port 4200 -c stg",
    "prd": "ng serve --port 4200 -c prd",
    "build-dev": "ng build -c dev",
    "build-hml": "ng build -c stg --build-optimizer",
    "build-prd": "ng build -c prd --output-hashing=all",
    "electron-dev": "electron .",
    "electron-prd": "ng build && electron ."
  },
